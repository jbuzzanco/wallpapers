{
    "name": "electron-forge-project",
    "productName": "electron-forge-project-joel",
    "version": "1.0.0",
    "description": "Wallpaper setting app for visualizations",
    "main": "src/index.js",
    "icon": "src/icons/mac/eye-icon.icns",
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package --arch universal",
        "make": "electron-forge make --arch universal --platform mas",
        "sign": "electron-osx-sign MacWallpapers-darwin-x64/MacWallpapers.app",
        "publish": "electron-forge publish",
        "lint": "echo \"No linting configured\""
    },
    "keywords": [],
    "author": "JoelonDesign",
    "license": "MIT",
    "dependencies": {
        "@electron-forge/maker-pkg": "^7.2.0",
        "electron-squirrel-startup": "^1.0.0",
        "wallpaper": "^6.1.1"
    },
    "devDependencies": {
        "@electron-forge/cli": "^7.2.0",
        "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
        "electron": "25.3.0"
    },
    "config": {
        "forge": {
            "packagerConfig": {
                "appBundleId": "com.terriblegoodday.MacWallpapers",
                "platform": "mas",
                "icon": "src/icons/mac/icon-overlyvisual.icns",
                "app-category-type": "public.app-category.utilities",
                "osxSign": {
                    "identity": "Apple Distribution: Eduard Dzhumagaliev (HA8PQ78LFS)",
                    "hardened-runtime": true,
                    "entitlements": "entitlements.plist",
                    "entitlements-inherit": "entitlements.plist",
                    "gatekeeper-assess": false,
                    "provisioningProfile": "/Users/terriblegoodday/Downloads/MacWallpapers_Upwork_App_Store.provisionprofile"
                },
                "osxUniversal": {
                    "x64ArchFiles": "**/macos-wallpaper"
                },
                "overwrite": true,
                "name": "MacWallpapers",
                "executableName": "MacWallpapers",
                "appVersion": "1.0.0",
                "buildVersion": "1.0.108"
            },
            "makers": [
                {
                    "name": "@electron-forge/maker-pkg",
                    "platform": "mas",
                    "config": {
                        "identity": "3rd Party Mac Developer Installer: Eduard Dzhumagaliev (HA8PQ78LFS)"
                    }
                }
            ]
        }
    }
}
